<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.helpinghands.domain.Participant" table="Participants">
        <id name="id" column="id">
            <generator class="increment"/>
        </id>

        <property name="organizer" column="isOrganizer"/>

        <many-to-one name="voluntar" class="com.helpinghands.domain.Voluntar" fetch="select">
            <column name="idVoluntar" not-null="true" />
        </many-to-one>
    </class>
</hibernate-mapping>